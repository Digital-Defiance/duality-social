{
  "name": "duality-social",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "newSecret": "node -e \"console.log (require('crypto').randomBytes(32).toString ('hex'));\"",
    "build:all": "npx nx build duality-social-lib && npx nx build duality-social-angular && npx nx build duality-social-node",
    "build:all:verbose": "npx nx build duality-social-lib --verbose && npx nx build duality-social-angular --verbose && npx nx build duality-social-node --verbose",
    "build:all:dev": "npx nx build duality-social-lib --configuration development && npx nx build duality-social-angular --configuration development && npx nx build duality-social-node --configuration development",
    "build:node": "npx nx build duality-social-node",
    "build:node:dev": "npx nx build duality-social-node --configuration development",
    "test:all": "npx nx test duality-social-lib && npx nx test duality-social-angular && npx nx test duality-social-node",
    "build": "yarn build:all",
    "serve": "npx nx serve duality-social-node --configuration production",
    "serve:dev": "npx nx serve duality-social-node --configuration development",
    "serve:angular:dev": "npx nx serve duality-social-angular --configuration development",
    "serve:angular": "npx nx serve duality-social-angular --configuration production",
    "npm-globals": "npm install -g yarn @angular/cli @nrwl/cli npx nx jest",
    "azure:clear": "az account clear",
    "azure:login": "az login",
    "azure:allow-telemetry": "az config set core.allow_broker=true && az account clear && az login",
    "azure:auth-cycle": "az account clear && az login",
    "clean:all": "rm -rf node_modules apps/duality-social-angular/node_modules apps/duality-social-node/node_modules libs/duality-social-lib/node_modules dist/ ~/.cache && yarn cache clean",
    "migrate:latest": "npx nx migrate latest",
    "migrate:run-migrations": "npx nx migrate --run-migrations"
  },
